<head>


<link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">
</head>
<script type="application/json" id="bootstrapped_course_json">
  <%= @courses.to_json.html_safe %>
</script>
<script type="application/json" id="bootstrapped_current_user_json">
  <%= current_user.to_json.html_safe %>
</script>
<div class="container">
  <header>
    <div class="versal-logo"></div>
    <div class="user-log-in">
      <% if !current_user.nil? %>
        <ul>
          Logged in as: <%= current_user.username %></li>
          <%= button_to "Logout", session_url, :method => :delete %></li>
        </ul>
      <% else %>
      
      <ul>
        <form role="form" action="<%= session_url %>" method="post">
            <input
              name="authenticity_token"
              type="hidden"
              value="<%= form_authenticity_token %>">
            <label for="exampleInputEmail1" class="log-in-label">Email address</label>
            <input name="username" type="username" class="form-control" id="exampleInputEmail1" placeholder="Enter username" value="guest">
            <br>
            <label for="exampleInputPassword1" class="log-in-label">Password</label>
            <input name="password" type="password" class="form-control" id="exampleInputPassword1" placeholder="Password" value="helloworld">
            
          <button type="submit" class="btn btn-success">Log In</button>
        </form>
      </ul>
      <% end %>
    </div>
  </header>

  <div class="content-container">
    <div class="course-title-show"></div>
      <div class="course-show">
        <div class="sidebar">
        <!-- <div class="course-title-show"></div> -->
        <div class="lesson-title-show"></div>
      </div>
        <div class="view">
      <!-- <div class="sublesson-title-show"></div> -->
      <div class="content-show"></div>
    </div>
      </div>
  </div>

</div>

<footer><p>Â© 2014 Jessica J. Kim</p></footer>
